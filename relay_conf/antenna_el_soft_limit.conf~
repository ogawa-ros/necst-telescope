# Antenna

[Publishers]
flag:
  {
    'name': '/el_soft_limit',
    'data_class': std_msgs.msg.Bool,
    'latch': True,
    'queue_size': 1,
  }

cmd2:
  {
    'name': '/el_cmd2',
    'data_class': std_msgs.msg.Float64,
    'queue_size': 1,
  }

speed:
  {
    'name': '/el_speed',
    'data_class': std_msgs.msg.Float64,
    'latch': True,
    'queue_size': 1,
  }

[Subscribers]
cmd:
  {
    'name': '/el_cmd',
    'data_class': std_msgs.msg.Float64,
    'queue_size': 1,
  }

[Logic]
code:
    if 0 < cmd < 89: cmd2.publish(cmd); flag.publish(False)
    else: flag.publish(True); speed.publish(0.0)
